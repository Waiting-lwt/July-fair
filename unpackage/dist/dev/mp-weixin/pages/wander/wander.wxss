.wander .wander-top {
  padding: 20rpx 0;
  width: 730rpx;
  position: relative;
  overflow: hidden;
  margin: 0 auto;
}
.wander .wander-top .search {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  line-height: 50rpx;
  width: 700rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.wander .wander-top .search .icon-fangdajing {
  font-size: 50rpx;
  color: #4F73A5;
  margin-top: -2rpx;
}
.wander .wander-top .search .Search {
  color: #649DD4;
  font-size: 50rpx;
}
.wander .wander-top .search input[type=text] {
  width: 70%;
  height: 100%;
  -webkit-transform: all 1s ease;
          transform: all 1s ease;
  padding: 0 0 10rpx 0;
  box-sizing: border-box;
  border-bottom: 2rpx solid transparent;
}
.wander .wander-top .search .inputActive {
  border-bottom: 2rpx solid #4F73A5 !important;
}
.wander .wander-top .search .location text:nth-child(2) {
  color: #649DD4;
}
.wander .wander-top .container {
  margin-top: 30rpx;
  width: 100%;
}
.wander .wander-top .container .swiper {
  position: relative;
  top: 20rpx;
  width: 100%;
  height: 400rpx;
  border-radius: 24rpx;
  overflow: hidden;
  margin: 0 auto;
}
.wander .wander-top .container .swiper swiper-item {
  width: 100%;
}
.wander .wander-top .container .swiper swiper-item .swiper-item {
  margin: 0 auto;
  text-align: center;
  background-color: #FFFFFF;
  width: 690rpx;
  border-radius: 24rpx;
  border: 1px solid #DEDDDA;
  box-shadow: 5rpx 5rpx 20rpx #cececb;
}
.wander .wander-top .container .swiper swiper-item .swiper-item image {
  width: 690rpx;
  height: 360rpx;
  border-radius: 24rpx;
}
.wander .wander-top .container .swiper swiper-item .swiper-item .description {
  font-size: 24rpx;
  line-height: 50rpx;
}
.wander .wander-top .container .swiper-dots {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin-top: 30rpx;
}
.wander .wander-top .container .swiper-dots .dot {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background-color: #DEDDDA;
  margin-right: 20rpx;
  -webkit-transition: all 0.2s;
  transition: all 0.2s;
}
.wander .wander-top .container .swiper-dots .active {
  width: 40rpx;
  border-radius: 8rpx;
  background-color: #4F73A5;
}
